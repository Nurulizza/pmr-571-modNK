<?xml version='1.0'?>
<model name="ERK_hatakeyama" xmlns="http://www.cellml.org/cellml/1.0#" xmlns:cellml="http://www.cellml.org/cellml/1.0#">
    <!--
*******************************************************
***         U   N   I   T   S                       ***
*******************************************************
-->
    <units name="s">
        <unit units="second"/>
    </units>
    <units name="nM">
        <unit prefix="nano" units="mole"/>
        <unit exponent="-1" units="litre"/>
    </units>
    <units name="flux">
        <unit units="nM"/>
        <unit exponent="-1" units="s"/>
    </units>
    <units name="second_order_rate_constant">
        <unit exponent="-1" units="nM"/>
        <unit exponent="-1" units="s"/>
    </units>
    <units name="first_order_rate_constant">
        <unit exponent="-1" units="s"/>
    </units>
    <component name="ERK">
        <variable name="time" public_interface="in" units="s"/>
        <!-- Parameters-->
        <variable name="kf19" public_interface="in" units="first_order_rate_constant"/>
        <variable name="k19" public_interface="in" units="nM"/>
        <variable name="kf20" public_interface="in" units="first_order_rate_constant"/>
        <variable name="k20" public_interface="in" units="nM"/>
        <variable name="kf21" public_interface="in" units="first_order_rate_constant"/>
        <variable name="k21" public_interface="in" units="nM"/>
        <variable name="kf22" public_interface="in" units="first_order_rate_constant"/>
        <variable name="k22" public_interface="in" units="nM"/>
        <!-- Variables-->
        <variable name="MEKPP" public_interface="in" units="nM"/>
        <variable name="MKP3" public_interface="in" units="nM"/>
        <variable name="ERK" public_interface="in" units="nM"/>
        <variable name="ERKP" public_interface="in" units="nM"/>
        <variable name="ERKPP" public_interface="in" units="nM"/>
        <!--Constants-->
        <variable initial_value="1" name="one" units="dimensionless"/>
        <variable name="J_ERKPP" public_interface="out" units="flux"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
                <eq/>
                <ci>J_ERKPP</ci>
                <apply>
                    <minus/>
                    <apply>
                        <divide/>
                        <apply>
                            <times/>
                            <ci>kf21</ci>
                            <ci>MEKPP</ci>
                            <ci>ERKP</ci>
                        </apply>
                        <apply>
                            <plus/>
                            <apply>
                                <times/>
                                <ci>k21</ci>
                                <apply>
                                    <plus/>
                                    <ci>one</ci>
                                    <apply>
                                        <divide/>
                                        <ci>ERK</ci>
                                        <ci>k19</ci>
                                    </apply>
                                </apply>
                            </apply>
                            <ci>ERKP</ci>
                        </apply>
                    </apply>
                    <apply>
                        <divide/>
                        <apply>
                            <times/>
                            <ci>kf22</ci>
                            <ci>MKP3</ci>
                            <ci>ERKPP</ci>
                        </apply>
                        <apply>
                            <plus/>
                            <apply>
                                <times/>
                                <ci>k22</ci>
                                <apply>
                                    <plus/>
                                    <ci>one</ci>
                                    <apply>
                                        <divide/>
                                        <ci>ERKP</ci>
                                        <ci>k20</ci>
                                    </apply>
                                </apply>
                            </apply>
                            <ci>ERKPP</ci>
                        </apply>
                    </apply>
                </apply>
            </apply>
            <apply>
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>time</ci>
                    </bvar>
                    <ci>ERK</ci>
                </apply>
                <apply>
                    <plus/>
                    <apply>
                        <divide/>
                        <apply>
                            <times/>
                            <apply>
                                <minus/>
                                <ci>kf19</ci>
                            </apply>
                            <ci>MEKPP</ci>
                            <ci>ERK</ci>
                        </apply>
                        <apply>
                            <plus/>
                            <apply>
                                <times/>
                                <ci>k19</ci>
                                <apply>
                                    <plus/>
                                    <ci>one</ci>
                                    <apply>
                                        <divide/>
                                        <ci>ERKP</ci>
                                        <ci>k21</ci>
                                    </apply>
                                </apply>
                            </apply>
                            <ci>ERK</ci>
                        </apply>
                    </apply>
                    <apply>
                        <divide/>
                        <apply>
                            <times/>
                            <ci>kf20</ci>
                            <ci>MKP3</ci>
                            <ci>ERKP</ci>
                        </apply>
                        <apply>
                            <plus/>
                            <apply>
                                <times/>
                                <ci>k20</ci>
                                <apply>
                                    <plus/>
                                    <ci>one</ci>
                                    <apply>
                                        <divide/>
                                        <ci>ERKPP</ci>
                                        <ci>k22</ci>
                                    </apply>
                                </apply>
                            </apply>
                            <ci>ERKP</ci>
                        </apply>
                    </apply>
                </apply>
            </apply>
            <!--ode(ERKPP, time) = kf21*MEKPP*ERKP/(k21*(one+ERK/k19)+ERKP)-kf22*MKP3*ERKPP/(k22*(one+ERKP/k20)+ERKPP);-->
            <apply>
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>time</ci>
                    </bvar>
                    <ci>ERKP</ci>
                </apply>
                <apply>
                    <plus/>
                    <apply>
                        <minus/>
                        <apply>
                            <minus/>
                            <apply>
                                <divide/>
                                <apply>
                                    <times/>
                                    <ci>kf19</ci>
                                    <ci>MEKPP</ci>
                                    <ci>ERK</ci>
                                </apply>
                                <apply>
                                    <plus/>
                                    <apply>
                                        <times/>
                                        <ci>k19</ci>
                                        <apply>
                                            <plus/>
                                            <ci>one</ci>
                                            <apply>
                                                <divide/>
                                                <ci>ERKP</ci>
                                                <ci>k21</ci>
                                            </apply>
                                        </apply>
                                    </apply>
                                    <ci>ERK</ci>
                                </apply>
                            </apply>
                            <apply>
                                <divide/>
                                <apply>
                                    <times/>
                                    <ci>kf20</ci>
                                    <ci>MKP3</ci>
                                    <ci>ERKP</ci>
                                </apply>
                                <apply>
                                    <plus/>
                                    <apply>
                                        <times/>
                                        <ci>k20</ci>
                                        <apply>
                                            <plus/>
                                            <ci>one</ci>
                                            <apply>
                                                <divide/>
                                                <ci>ERKPP</ci>
                                                <ci>k22</ci>
                                            </apply>
                                        </apply>
                                    </apply>
                                    <ci>ERKP</ci>
                                </apply>
                            </apply>
                        </apply>
                        <apply>
                            <divide/>
                            <apply>
                                <times/>
                                <ci>kf21</ci>
                                <ci>MEKPP</ci>
                                <ci>ERKP</ci>
                            </apply>
                            <apply>
                                <plus/>
                                <apply>
                                    <times/>
                                    <ci>k21</ci>
                                    <apply>
                                        <plus/>
                                        <ci>one</ci>
                                        <apply>
                                            <divide/>
                                            <ci>ERK</ci>
                                            <ci>k19</ci>
                                        </apply>
                                    </apply>
                                </apply>
                                <ci>ERKP</ci>
                            </apply>
                        </apply>
                    </apply>
                    <apply>
                        <divide/>
                        <apply>
                            <times/>
                            <ci>kf22</ci>
                            <ci>MKP3</ci>
                            <ci>ERKPP</ci>
                        </apply>
                        <apply>
                            <plus/>
                            <apply>
                                <times/>
                                <ci>k22</ci>
                                <apply>
                                    <plus/>
                                    <ci>one</ci>
                                    <apply>
                                        <divide/>
                                        <ci>ERKP</ci>
                                        <ci>k20</ci>
                                    </apply>
                                </apply>
                            </apply>
                            <ci>ERKPP</ci>
                        </apply>
                    </apply>
                </apply>
            </apply>
        </math>
    </component>
</model>
