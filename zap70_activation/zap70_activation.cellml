<?xml version='1.0'?>
<model name="zap70_activation" xmlns="http://www.cellml.org/cellml/1.1#" xmlns:cellml="http://www.cellml.org/cellml/1.1#">
    <!--
*******************************************************
***         U   N   I   T   S                       ***
*******************************************************
-->
    <units name="s">
        <unit units="second"/>
    </units>
    <units name="nM">
        <unit prefix="nano" units="mole"/>
        <unit exponent="-1" units="litre"/>
    </units>
    <units name="first_order_rate_constant">
        <unit exponent="-1" units="s"/>
    </units>
    <units name="second_order_rate_constant">
        <unit exponent="-1" units="nM"/>
        <unit exponent="-1" units="s"/>
    </units>
    <units name="flux">
        <unit units="nM"/>
        <unit exponent="-1" units="s"/>
    </units>
    <!--
*********************************************************
***       C   O   M   P   O   N   E   N   T   S       ***
*********************************************************
-->
    <component name="environment">
        <variable name="time" public_interface="out" units="second"/>
    </component>
    <component name="zap70">
        <variable name="time" public_interface="in" units="second"/>
        <variable name="J1" units="flux"/>
        <variable name="J2" units="flux"/>
        <variable name="J3" units="flux"/>
        <variable name="J4" public_interface="out" units="flux"/>
        <variable name="J5" units="flux"/>
        <variable name="J21" public_interface="in" units="flux"/>
        <variable name="k_f1" public_interface="in" units="second_order_rate_constant"/>
        <variable name="k_r1" public_interface="in" units="second_order_rate_constant"/>
        <variable name="k_f2" public_interface="in" units="second_order_rate_constant"/>
        <variable name="k_r2" public_interface="in" units="first_order_rate_constant"/>
        <variable name="k_f3" public_interface="in" units="second_order_rate_constant"/>
        <variable name="k_r3" public_interface="in" units="second_order_rate_constant"/>
        <variable name="k_f4" public_interface="in" units="first_order_rate_constant"/>
        <variable name="k_r4" public_interface="in" units="second_order_rate_constant"/>
        <variable name="k_f5" public_interface="in" units="first_order_rate_constant"/>
        <variable name="itam" public_interface="in" units="nM"/>
        <variable name="plck" public_interface="in" units="nM"/>
        <variable name="zap70" public_interface="in" units="nM"/>
        <variable initial_value="0" name="pitam" units="nM"/>
        <variable name="lck" public_interface="in" units="nM"/>
        <variable initial_value="0" name="pitamzap70" units="nM"/>
        <variable initial_value="0" name="pitampzap70" units="nM"/>
        <variable name="pzap70" public_interface="in" units="nM"/>
        <variable initial_value="0" name="pitamp2zap70" units="nM"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
                <eq/>
                <ci>J1</ci>
                <apply>
                    <minus/>
                    <apply>
                        <times/>
                        <ci>k_f1</ci>
                        <ci>itam</ci>
                        <ci>plck</ci>
                    </apply>
                    <apply>
                        <times/>
                        <ci>k_r1</ci>
                        <ci>pitam</ci>
                        <ci>lck</ci>
                    </apply>
                </apply>
            </apply>
            <apply>
                <eq/>
                <ci>J2</ci>
                <apply>
                    <minus/>
                    <apply>
                        <times/>
                        <ci>k_f2</ci>
                        <ci>pitam</ci>
                        <ci>zap70</ci>
                    </apply>
                    <apply>
                        <times/>
                        <ci>k_r2</ci>
                        <ci>pitamzap70</ci>
                    </apply>
                </apply>
            </apply>
            <apply>
                <eq/>
                <ci>J3</ci>
                <apply>
                    <minus/>
                    <apply>
                        <times/>
                        <ci>k_f3</ci>
                        <ci>pitamzap70</ci>
                        <ci>plck</ci>
                    </apply>
                    <apply>
                        <times/>
                        <ci>k_r3</ci>
                        <ci>pitampzap70</ci>
                        <ci>lck</ci>
                    </apply>
                </apply>
            </apply>
            <apply>
                <eq/>
                <ci>J4</ci>
                <apply>
                    <minus/>
                    <apply>
                        <times/>
                        <ci>k_f4</ci>
                        <ci>pitampzap70</ci>
                    </apply>
                    <apply>
                        <times/>
                        <ci>k_r4</ci>
                        <ci>pitam</ci>
                        <ci>pzap70</ci>
                    </apply>
                </apply>
            </apply>
            <apply>
                <eq/>
                <ci>J5</ci>
                <apply>
                    <times/>
                    <ci>k_f5</ci>
                    <ci>pitampzap70</ci>
                </apply>
            </apply>
            <apply>
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>time</ci>
                    </bvar>
                    <ci>itam</ci>
                </apply>
                <apply>
                    <minus/>
                    <ci>J21</ci>
                    <ci>J1</ci>
                </apply>
            </apply>
            <apply>
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>time</ci>
                    </bvar>
                    <ci>pitam</ci>
                </apply>
                <apply>
                    <plus/>
                    <apply>
                        <minus/>
                        <ci>J1</ci>
                        <ci>J2</ci>
                    </apply>
                    <ci>J4</ci>
                </apply>
            </apply>
            <apply>
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>time</ci>
                    </bvar>
                    <ci>pitamzap70</ci>
                </apply>
                <apply>
                    <minus/>
                    <ci>J2</ci>
                    <ci>J3</ci>
                </apply>
            </apply>
            <apply>
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>time</ci>
                    </bvar>
                    <ci>pitampzap70</ci>
                </apply>
                <apply>
                    <minus/>
                    <apply>
                        <minus/>
                        <ci>J3</ci>
                        <ci>J4</ci>
                    </apply>
                    <ci>J5</ci>
                </apply>
            </apply>
            <apply>
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>time</ci>
                    </bvar>
                    <ci>pitamp2zap70</ci>
                </apply>
                <ci>J5</ci>
            </apply>
        </math>
    </component>
    <connection>
        <map_components component_1="zap70" component_2="environment"/>
        <map_variables variable_1="time" variable_2="time"/>
    </connection>
</model>
